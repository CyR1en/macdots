{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\ueb05 ",
          "foreground": "#f5bde6",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "folder"
          },
          "template": " {{ .Path }}",
          "foreground": "lightCyan",
          "type": "path",
          "style": "plain"
        },
        {
          "properties": {
            "branch_icon": "",
            "cache_duration": "none"
          },
          "template": " <#DDB15F>git(</>{{ .HEAD }}<#DDB15F>)</>",
          "foreground": "magenta",
          "type": "git",
          "style": "plain"
        },
	{
          "properties": {
            "cache_duration": "none"
          },
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }}{{ end }}{{ end }} ",
          "foreground": "#906cff",
          "type": "python",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": " âžœ",
          "foreground": "green",
          "type": "text",
          "style": "plain"
        }
      ]
    }
  ],
  "version": 3,
  "final_space": true
}
